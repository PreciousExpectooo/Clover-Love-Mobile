{
  "expo": {
    "name": "Clover",
    "slug": "clover-love",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#640d6b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cloverlove.cloverlove"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cloverlove.cloverlove"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d441cf1b-de38-4370-b88e-a4eead83ff8d"
      }
    },
    "owner": "clover-love",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d441cf1b-de38-4370-b88e-a4eead83ff8d"
    },
    "newArchEnabled": true
  }
}
